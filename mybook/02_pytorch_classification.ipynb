   "source": [
    "# 정확도 계산 (분류 지표)\n",
    "def accuracy_fn(y_true, y_pred):\n",
    "    correct = torch.eq(y_true, y_pred).sum().item() # torch.eq()는 두 텐서가 같은 위치를 계산함\n",
    "    acc = (correct / len(y_pred)) * 100 \n",
    "    return acc"
   ]
